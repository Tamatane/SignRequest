{
  "name": "atanenl/signrequest",
  "description": "SignRequest API Client",
  "keywords": [
    "SignRequest"
  ],
  "homepage": "https://www.kidskonnect.nl",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Tamasja",
      "email": "tamasja@atane.nl"
    },
    {
      "name": "DaniÃ«l Oosterhuis",
      "email": "daniel@atane.nl"
    }
  ],
  "require": {
    "php": "^7.4|^8.0.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.9"
  },
  "autoload": {
    "psr-0": {
      "AtaneNL\\SignRequest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AtaneNL\\SignRequest\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
